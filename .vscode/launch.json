  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Go API",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "api/cmd/server",
      "args": ["serve", "--dir", "${workspaceFolder}/api/pb_data"],
      "cwd": "${workspaceFolder}/api"
    },
    {
      "name": "Launch Go Download",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "api/cmd/downloader",
      "cwd": "${workspaceFolder}/api",
      "args": [
        "-vv",
        "-u",
        "https://shutterbase.mxcd.de/api/v1",
        "-k",
        "<INSERT API KEY>",
        "-p",
        "c3f2ea22-2e15-44d6-a926-69db76a60c86",
        "download",
        "delta",
        "vbo"
      ]
    },
    {
      "name": "Launch Go Seed",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "api/internal/cmd/seed"
    }
  ]
}
